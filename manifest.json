{
	"manifest_version": 2,
	"name": "DofusDbToDofusNoobsExt",
	"description": "DofusDbToDofusNoobsExt: redirection vers DofusNoobs depuis DofusDb",
	"version": "1.0.8",
	"browser_action": {
		"default_icon": "images/icon.png"
	},
	"icons": {
		"128": "images/icon.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"run_at": "document_end",
			"matches": ["*://dofusdb.fr/*"],
			"js": ["content.js"]
		}
	]
}
