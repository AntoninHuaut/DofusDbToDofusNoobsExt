{
	"manifest_version": 2,
	"name": "DofusDbToDofusNoobsExt",
	"description": "DofusDbToDofusNoobsExt: redirection vers DofusNoobs depuis DofusDb",
	"version": "1.1.0",
	"permissions": ["storage", "tabs"],
	"browser_action": {
		"default_icon": "images/icon.png"
	},
	"icons": {
		"128": "images/icon.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"run_at": "document_end",
			"matches": ["*://dofusdb.fr/*"],
			"js": ["content.js"]
		}
	],
	"browser_specific_settings": {
		"gecko": {
			"update_url": "https://raw.githubusercontent.com/AntoninHuaut/DofusDbToDofusNoobsExt/refs/heads/master/update_firefox.json"
		}
	},
	"options_page": "options/options.html",
	"options_ui": {
		"page": "options/options.html"
	}
}
